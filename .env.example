# Example environment file

# Application settings
APP_NAME=PipelineBuilder
LOG_LEVEL=INFO

# Database URLs
DATABASE_URL=postgresql://user:password@postgres:5432/pipelinebuilder
REDIS_URL=redis://redis:6379/0

# OpenAI API settings
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o
EMBEDDING_MODEL=text-embedding-3-large

# Prompt budgets (token counts)
PROMPT_BUDGET_SYSTEM=500
PROMPT_BUDGET_USER_CTX=1000
PROMPT_BUDGET_SNIPPETS=500

## Additional settings can be added as needed
